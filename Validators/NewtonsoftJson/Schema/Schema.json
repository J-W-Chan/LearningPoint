{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "CreateUserRequest": {
      "type": "object",
      "properties": {
        "UserName": {
          "type": "string",
          "minLength": 5
        },
        "DisplayName": {
          "type": "string",
          "minLength": 5
        }
      },
      "required": [ "UserName", "DisplayName" ]
    },
    "CreateProductRequest": {
      "type": "object",
      "properties": {
        "ProductId": {
          "type": "string",
          "minLength": 1
        },
        "ProductName": {
          "type": "string",
          "minLength": 1
        },
        "Price": {
          "type": "number",
          "minimum": 0
        }
      },
      "required": [ "ProductId", "ProductName", "Price" ]
    }
  }
}
