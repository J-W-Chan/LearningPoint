<Window x:Class="FodyUnusualBehavior.Views.ShellView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FodyUnusualBehavior.Views"
        mc:Ignorable="d"
        Title="ShellView" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="{Binding RandomNumber}" FontSize="16" HorizontalAlignment="Center"/>
            <Button Content="Randomize" x:Name="Randomize"/>
        </StackPanel>
        
    </Grid>
</Window>
